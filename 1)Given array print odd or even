#include<stdio.h>
int main()
{
	int num[10],i;
	printf("enter the numbers");
	for(i=0;i<10;i++)
	
	scanf("%d",&num[i]);
	printf("\neven numbers");
	for(i=0;i<10;i++)
	{
	if (num[i]%2==0)
	printf("%d",num[i]);
	}
	printf("odd numbers");
	for(i=1;i<10;i++)
	{
		if (num[i]%2==1)
		printf("%d",num[i]); 
	}
	return 0;
		
	
}
